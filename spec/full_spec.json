{
    "components": {
        "securitySchemes": {
            "ApiKeyAuth": {
                "name": "X-API-Key",
                "in": "header",
                "type": "apiKey"
            }
        },
        "schemas": {
            "PetType": {
                "enum": [
                    "dog",
                    "cat"
                ],
                "type": "string"
            },
            "Pet": {
                "properties": {
                    "type": {
                        "$ref": "#/components/schemas/PetType"
                    },
                    "tag": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "id": {
                        "type": "number",
                        "readOnly": true
                    }
                },
                "required": [
                    "id",
                    "name",
                    "type"
                ]
            },
            "Error": {
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "code": {
                        "format": "int32",
                        "type": "integer"
                    }
                },
                "required": [
                    "code",
                    "message"
                ]
            }
        }
    },
    "paths": {
        "/ping2": {
            "get": {
                "x-eov-operation-handler": "../routes/ping2",
                "x-eov-operation-id": "ping2",
                "operationId": "ping2",
                "responses": {
                    "default": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        },
                        "description": "unexpected error"
                    },
                    "200": {
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "example": "pong",
                                    "type": "string"
                                }
                            }
                        },
                        "description": "OK"
                    }
                },
                "description": "ping2 then pong!"
            }
        },
        "/ping": {
            "get": {
                "responses": {
                    "default": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        },
                        "description": "unexpected error"
                    },
                    "200": {
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "example": "pong",
                                    "type": "string"
                                }
                            }
                        },
                        "description": "OK"
                    }
                },
                "x-eov-operation-handler": "../routes/ping",
                "x-eov-operation-id": "ping",
                "operationId": "ping",
                "description": "ping then pong!"
            }
        },
        "/getRecipesByCategory": {
            "get": {
                "x-eov-operation-handler": "../routes/getRecipesByCategory",
                "x-eov-operation-id": "getRecipesByCategory",
                "operationId": "getRecipesByCategory",
                "responses": {
                    "default": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        },
                        "description": "unexpected error"
                    },
                    "404": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "object"
                                    },
                                    "type": "string"
                                }
                            }
                        },
                        "description": "No recipes found for given category. The category could be an invalid category."
                    },
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "object"
                                    },
                                    "type": "array"
                                }
                            }
                        },
                        "description": "OK"
                    }
                },
                "parameters": [
                    {
                        "schema": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        },
                        "style": "form",
                        "required": true,
                        "description": "List of categories that should be used to search",
                        "name": "list",
                        "in": "query"
                    }
                ],
                "description": "Get the recipes that have the given categories associated with them."
            }
        },
        "/blah/ping3": {
            "get": {
                "x-eov-operation-handler": "../routes/blah/ping3",
                "x-eov-operation-id": "ping3",
                "operationId": "ping3",
                "responses": {
                    "default": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        },
                        "description": "unexpected error"
                    },
                    "200": {
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "example": "pong",
                                    "type": "string"
                                }
                            }
                        },
                        "description": "OK"
                    }
                },
                "description": "ping3 then pong!"
            }
        }
    },
    "servers": [
        {
            "url": "/v1"
        }
    ],
